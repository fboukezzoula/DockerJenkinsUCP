json_url="http://github.com/monrepo/myjsonfile.json"
json_data=$(curl -s "$json_url")

# Utilisez jq pour extraire les noms d'utilisateur du JSON
usernames=($(echo "$json_data" | jq -r 'keys[]'))

# Parcourez les noms d'utilisateur et ex√©cutez les commandes
for username in "${usernames[@]}"; do
    useradd "$username"
done
